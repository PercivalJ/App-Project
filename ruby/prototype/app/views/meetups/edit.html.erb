<h1>Edit this Meetup</h1>

<%= form_for @meetup do |f| %>
  <div>
    <%= f.label :festival %>
    <%= f.text_field :festival %>
  </div>

  <div>
    <%= f.label :location %>
    <%= f.text_field :location %>
  </div>

  <div>
    <%= f.label :time %>
    <%= f.text_field :time %>
  </div>

 
  <div>
    <%= f.submit %>
  </div>

<% end %>

<%= link_to "Back to the list!", meetups_path %>