<h1>Add New Meetups</h1>

<%= form_for [@festival, @meetup] do |f| %>

  <div>
    <%= f.label :location %>
    <%= f.text_field :location %>
  </div>

  <div>
    <%= f.label :time %>
    <%= f.text_field :time %>
  </div>

 
  <div>
    <%= f.submit "Add new Meetup!" %>
  </div>

<% end %>

<%= link_to "Back to the list!", festival_meetups_path %>